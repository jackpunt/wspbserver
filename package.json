{
  "name": "wspbserver",
  "version": "1.2.6",
  "description": "WebSocket Protobuf Server",
  "main": "dist/out-tsc/index.js",
  "files": [
    "dist/out-tsc"
  ],
  "scripts": {
    "patch-serial": "replace 'serializeBinary(): Uint8Array { throw new Error(\"Method not implemented.\"); }' 'serializeBinary(): Uint8Array { return this.serialize(); }' -- src/*Proto.ts",
    "patch-return": "replace 'return writer.getResultBuffer();' 'return writer.getResultBuffer();\n        return undefined;' -- src/*Proto.ts",
    "patch-reader": "replace -s 'Uint8Array ? new pb_1.BinaryReader(bytes) : bytes' 'pb_1.BinaryReader ? bytes : new pb_1.BinaryReader(bytes)' --  src/*Proto.ts",
    "patch": "npm run patch-reader; npm run patch-return; npm run patch-serial",
    "protojs": "protoc -I src/proto --js_out=import_style=commonjs,binary:. src/proto/*.proto",
    "protots": "protoc -I src/proto --ts_out=src src/proto/*.proto",
    "echo": "node dist/out-tsc/echoserver.js",
    "cgserve": "node dist/out-tsc/cg-server.js",
    "testns": "jest --runInBand --force-exit --detectOpenHandles --testPathIgnorePatterns=wspbserver.spec",
    "testwss": "jest --runInBand --force-exit --detectOpenHandles --verbose -t 'wss:'",
    "test": "jest --runInBand --force-exit --detectOpenHandles --verbose",
    "tsc-options": "are configured in tsconfig.json",
    "tsc": "tsc",
    "clean": "rm -f dist/out-tsc/*",
    "build": "npm run clean; npm run protots; npm run patch; npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackpunt/wspbserver.git"
  },
  "keywords": [
    "websocket",
    "protobuf",
    "server"
  ],
  "author": "Jeff Peck <jpeckj@gmail.com> (http://thegraid.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackpunt/wspbserver/issues"
  },
  "homepage": "https://github.com/jackpunt/wspbserver#readme",
  "dependencies": {
    "@thegraid/ezpromise": "^1.0.4",
    "chrome-launcher": "^0.13.4",
    "google-protobuf": "*",
    "moment": "^2.29.1",
    "tslib": "^2.1.0",
    "ws": "^7.4.4",
    "wspbclient": "^1.2.11"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/ws": "^7.4.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
