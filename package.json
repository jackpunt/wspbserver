{
  "name": "wspbserver",
  "version": "1.3.2",
  "description": "WebSocket Protobuf Server",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "echo": "node dist/echoserver.js",
    "cgserver": "node dist/cg-server.js",
    "testns": "jest --runInBand --force-exit --detectOpenHandles --testPathIgnorePatterns=wspbserver.spec",
    "testwss": "jest --runInBand --force-exit --detectOpenHandles --verbose -t 'wss:'",
    "test": "jest --runInBand --force-exit --detectOpenHandles --verbose",
    "tsc-options": "are configured in tsconfig.json",
    "tsc": "tsc",
    "clean": "rm -f dist/*",
    "reserve": "npm run build; npm run cgserver",
    "build": "npm run clean; npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackpunt/wspbserver.git"
  },
  "keywords": [
    "websocket",
    "protobuf",
    "server"
  ],
  "author": "Jeff Peck <jpeckj@gmail.com> (http://thegraid.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackpunt/wspbserver/issues"
  },
  "homepage": "https://github.com/jackpunt/wspbserver#readme",
  "dependencies": {
    "@thegraid/ezpromise": "^1.0.4",
    "google-protobuf": "*",
    "moment": "^2.29.1",
    "tslib": "^2.1.0",
    "ws": "^7.4.4",
    "wspbclient": "^1.2.11"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/ws": "^7.4.1",
    "chrome-launcher": "^0.13.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
