{
  "name": "@thegraid/wspbserver",
  "version": "1.5.8",
  "description": "WebSocket Protobuf Server for Client-Group proto",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "pub": "npm run build; npm version ${1:-patch}; npm publish --access public",
    "echo": "node dist/echoserver.js",
    "cgserver": "node dist/cg-server.js",
    "testns": "jest --runInBand --force-exit --detectOpenHandles --testPathIgnorePatterns=wspbserver.spec",
    "testwss": "jest --runInBand --force-exit --detectOpenHandles --verbose -t 'wss:'",
    "test": "jest --runInBand --force-exit --detectOpenHandles --verbose ",
    "Note1": "to change hostname use: 'host=game5 npm test' ",
    "Note2": "jest 27 complains about the open listener in wss-listener.ts [26 does not *complain*]",
    "tsc-options": "are configured in tsconfig.json",
    "tsc": "tsc",
    "clean": "rm -f dist/*",
    "reserve": "date; npm run build; npm run cgserver",
    "build": "npm run clean; npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackpunt/wspbserver.git"
  },
  "keywords": [
    "websocket",
    "protobuf",
    "server"
  ],
  "author": "Jeff Peck <jpeckj@gmail.com> (http://thegraid.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackpunt/wspbserver/issues"
  },
  "homepage": "https://github.com/jackpunt/wspbserver#readme",
  "dependencies": {
    "@thegraid/ezpromise": "^1.0.4",
    "@thegraid/wspbclient": "^1.4.3",
    "google-protobuf": "*",
    "moment": "^2.29.1",
    "tslib": "^2.1.0",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/google-protobuf": "*",
    "@types/node": "*",
    "@types/ws": "^7.4.1",
    "chrome-launcher": "^0.13.4",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4"
  }
}
